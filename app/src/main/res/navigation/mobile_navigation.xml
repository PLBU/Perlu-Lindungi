<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_news">

    <fragment
        android:id="@+id/navigation_bookmark"
        android:name="com.example.perlulindungi.ui.bookmark.BookmarkFragment"
        android:label="@string/title_bookmark"
        tools:layout="@layout/fragment_bookmark" />

    <fragment
        android:id="@+id/navigation_news"
        android:name="com.example.perlulindungi.ui.news.NewsFragment"
        android:label="@string/title_news"
        tools:layout="@layout/fragment_news" >
    </fragment>

    <fragment
        android:id="@+id/navigation_news_web_view"
        android:name="com.example.perlulindungi.ui.news.NewsWebViewFragment"
        android:label="@string/title_news"
        tools:layout="@layout/fragment_news_web_view" />

    <fragment
        android:id="@+id/navigation_lokasi_vaksin"
        android:name="com.example.perlulindungi.ui.lokasi_vaksin.LokasiVaksinFragment"
        android:label="@string/title_lokasi_vaksin"
        tools:layout="@layout/fragment_lokasi_vaksin" />

    <fragment
        android:id="@+id/navigation_lokasi_vaksin_detail"
        android:name="com.example.perlulindungi.ui.lokasi_vaksin.LokasiVaksinDetailFragment"
        android:label="@string/title_lokasi_vaksin_detail"
        tools:layout="@layout/fragment_lokasi_vaksin_detail" />
</navigation>